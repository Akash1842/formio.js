<select
  ref="{{ctx.input.ref ? ctx.input.ref : 'selectContainer'}}"
  {{ ctx.input.multiple ? 'multiple' : '' }}
  {% for (var attr in ctx.input.attr) { %}
  {{attr}}="{{ctx.input.attr[attr]}}"
  {% } %}

  aria-label="{{ctx.input.ref === 'selectContainer' || !ctx.input.ref ? ctx.input.component.label : ctx.input.ref}}"
  aria-describedby="desc-{{ctx.component.key}}"
  aria-required="{{ctx.input.ref === 'selectContainer' || !ctx.input.ref ? ctx.input.component.validate.required : ctx.input.component.fields[ctx.input.ref].required}}"
>{{ctx.selectOptions}}</select>

{% if (ctx.input.component.widget && ctx.input.component.widget === 'html5') { %}
<input type="text"
       class="formio-select-autocomplete-input"
       ref="autocompleteInput"
       {% if (ctx.input.attr.autocomplete) { %}
       autocomplete="{{ctx.input.attr.autocomplete}}"
       {% } %}
       tabindex="-1"
/>
{% } %}